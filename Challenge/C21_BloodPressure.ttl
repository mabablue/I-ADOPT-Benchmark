@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/iadopt/challenge/> .

ex:C21
    a 
        iop:Variable ;
    rdfs:label 
        "Resting systolic blood pressure" ;
    rdfs:comment 
        """Resting systolic blood pressure in millimeters of mercury (mmHg)""" ;
    iop:hasObjectOfInterest 
        <http://purl.obolibrary.org/obo/UBERON_0000178> ;
    iop:hasMatrix 
        <http://purl.obolibrary.org/obo/NCIT_C14225> ;
    iop:hasProperty 
        <http://qudt.org/vocab/quantitykind/Pressure> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "condition: resting" ;
             iop:constrains <http://purl.obolibrary.org/obo/NCIT_C14225> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "state: systolic" ;
             iop:constrains <http://qudt.org/vocab/quantitykind/Pressure> ;
        ] .

<http://purl.obolibrary.org/obo/UBERON_0000178>
    a 
        iop:Entity ;
    rdfs:label 
        "blood" .

<http://purl.obolibrary.org/obo/NCIT_C14225>
    a 
        iop:Entity ;
    rdfs:label 
        "human" .

<http://qudt.org/vocab/quantitykind/Pressure>
    a 
        iop:Property ;
    rdfs:label 
        "pressure" .
