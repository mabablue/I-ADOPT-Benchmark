@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/iadopt/challenge/> .

ex:C30
    a 
        iop:Variable ;
    rdfs:label 
        "Date of last PCR-test" ;
    rdfs:comment 
        """date of last COVID-19 PCR test performed by a certified laboratory""" ;
    iop:hasObjectOfInterest 
        _:b0 ;
    iop:hasMatrix 
        <http://purl.obolibrary.org/obo/NCIT_C16960> ;
    iop:hasStatisticalModifier 
        <http://www.ontology-of-units-of-measure.org/resource/om-2/last> ;
    iop:hasProperty 
        <http://www.ontology-of-units-of-measure.org/resource/om-2/Date> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "method: performed by a certified laboratory" ;
             iop:constrains _:b0 ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "type: SARS-CoV2 " ;
             iop:constrains _:b0 ;
        ] .

_:b0
    a 
        iop:Entity ;
    rdfs:label 
        "PCR-test" .

<http://purl.obolibrary.org/obo/NCIT_C16960>
    a 
        iop:Entity ;
    rdfs:label 
        "patient" .

<http://www.ontology-of-units-of-measure.org/resource/om-2/last>
    a 
        iop:StatisticalModifier ;
    rdfs:label 
        "last" .

<http://www.ontology-of-units-of-measure.org/resource/om-2/Date>
    a 
        iop:Property ;
    rdfs:label 
        "date" .
