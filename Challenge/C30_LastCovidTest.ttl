    iop:hasObjectOfInterest 
        <https://www.wikidata.org/wiki/Q104462307> ;
    iop:hasMatrix 
        <https://www.wikidata.org/wiki/Q181600> ;
    iop:hasProperty 
        <https://www.wikidata.org/wiki/Q205892> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "method: performed by a certified laboratory" ;
